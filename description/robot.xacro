<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"  name="robot">

    <!-- Example link -->
    <link name="base_link"></link>

    <joint name="base_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="chasis_link"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>

    <joint name="arm_left_front_joint" type="fixed">
        <origin xyz="-0.15 -0.0955 0.1" rpy="0.0 0.0 -1.5708"/>
        <parent link="chasis_link"/>
        <child link="arm_left_front_link"/>        
        <axis xyz="0.0 0.0 0.0" />
    </joint>

    <joint name="arm_left_back_joint" type="fixed">
        <origin xyz="0.17 -0.0955 0.1" rpy="3.14 0.0 -1.5708"/>
        <parent link="chasis_link"/>
        <child link="arm_left_back_link"/>        
        <axis xyz="0.0 0.0 0.0" />
    </joint>

    <joint name="arm_right_front_joint" type="fixed">
        <origin xyz="-0.15 0.0955 0.1" rpy="0.0 3.14  -1.5708"/>
        <parent link="chasis_link"/>
        <child link="arm_right_front_link"/>        
        <axis xyz="0.0 0.0 0.0" />
    </joint>

    <joint name="arm_right_back_joint" type="fixed">
        <origin xyz="0.17 0.0955 0.1" rpy="3.14 3.14  -1.5708"/>
        <parent link="chasis_link"/>
        <child link="arm_right_back_link"/>        
        <axis xyz="0.0 0.0 0.0" />
    </joint>


    <link name="chasis_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="20.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0115 0.0" rpy="0.0 0.0 -1.5708"/>
            <geometry>
                <mesh filename="file://$(find rover)/description/stl/Chasis.stl" scale="0.01 0.01 0.01"/>/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.6 0.3 0.2"/>
            </geometry>
        </collision>
    </link>

    <link name="arm_left_front_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="file://$(find rover)/description/stl/Arm_left_front.stl" scale="0.01 0.01 0.01"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.0 0.0 0.0"/>
            </geometry>
        </collision>
    </link>

    <link name="arm_left_back_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
